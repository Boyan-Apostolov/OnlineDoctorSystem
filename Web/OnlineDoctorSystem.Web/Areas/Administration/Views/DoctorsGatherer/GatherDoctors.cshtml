@{
    this.ViewData["Title"] = "Добавяне на лекари";
}
<h1 class="text-center">@this.ViewData["Title"]</h1>
<form method="post">
    <div class="form-group">
        <label for="count" class="form-control text-center">Брой лекари</label>
        <input id="count" name="count" type="number" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="townId" class="form-control text-center">ID на град (1-28)</label>
        <input id="townId" name="townId" type="number" class="form-control"/>
    </div>
    <button class="btn btn-success" type="submit">Добави</button>

</form>
